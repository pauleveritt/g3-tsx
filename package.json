{
  "name": "g3-tsx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve --watch",
    "dev:webstorm": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --input=sites/webstorm/guide --serve --watch",
    "dev:pycharm": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --input=sites/pycharm/guide --serve --watch",
    "incremental": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --incremental --serve --watch",
    "debug-start": "DEBUG=Eleventy* node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve --watch",
    "debug-start-windows": "set DEBUG=Eleventy* & node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve --watch",
    "build": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js && npm run assets",
    "debug-build": "DEBUG=Eleventy* node --require esbuild-register node_modules/@11ty/eleventy/cmd.js",
    "benchmark-build": "DEBUG=Eleventy:Benchmark* node --require esbuild-register node_modules/@11ty/eleventy/cmd.js",
    "clean": "rimraf _site && rimraf .11ty-vite",
    "preview": "vite preview",
    "assets": "npm run assets:pycharm && npm run assets:webstorm",
    "assets:pycharm": "copyfiles --flat _site/assets/* _site/pycharm/guide/assets",
    "assets:webstorm": "copyfiles --flat _site/assets/* _site/webstorm/guide/assets"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11ty/eleventy-img": "^2.0.1",
    "@types/11ty__eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-vite": "^2.0.0-canary.3",
    "@sinclair/typebox": "^0.25.21",
    "@testing-library/dom": "^8.20.0",
    "@types/markdown-it": "^12.2.3",
    "@types/vhtml": "^2.2.4",
    "@types/video.js": "^7.3.50",
    "@vitest/coverage-c8": "^0.28.4",
    "copyfiles": "^2.4.1",
    "esbuild-register": "^3.3.2",
    "happy-dom": "^8.2.2",
    "@types/luxon": "^3.2.0",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.3",
	"rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "vhtml": "^2.2.0",
    "vite": "^4.0.4",
    "vitest": "^0.28.3"
  },
  "volta": {
    "node": "19.6.0"
  }
}
