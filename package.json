{
  "name": "g3-tsx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:pycharm": "cd sites/pycharm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js --serve --watch",
    "build:pycharm": "npm run clean && npm run test && cd sites/pycharm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js --pathprefix=/pycharm/guide ",
    "dev:webstorm": "cd sites/webstorm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js --serve --watch",
    "build:webstorm": "npm run clean && npm run test && cd sites/webstorm-guide && node --require esbuild-register ../../node_modules/@11ty/eleventy/cmd.js --config=../../eleventy.config.js",
    "clean": "rimraf .11ty-vite sites/pycharm-guide/_site sites/pycharm-guide/.11ty-vite sites/webstorm-guide/_site",
    "test": "vitest run",
    "COMMENT": "// TODO DO WE NEED THESE SCRIPTS BELOW?",
    "incremental": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --incremental --serve --watch",
    "debug-start": "DEBUG=Eleventy* node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve --watch",
    "debug-start-windows": "set DEBUG=Eleventy* & node --require esbuild-register node_modules/@11ty/eleventy/cmd.js --serve --watch",
    "build": "node --require esbuild-register node_modules/@11ty/eleventy/cmd.js && npm run assets",
    "debug-build": "DEBUG=Eleventy* node --require esbuild-register node_modules/@11ty/eleventy/cmd.js",
    "benchmark-build": "DEBUG=Eleventy:Benchmark* node --require esbuild-register node_modules/@11ty/eleventy/cmd.js",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@sinclair/typebox": "^0.25.24",
    "@testing-library/dom": "^8.20.0",
    "@types/luxon": "^3.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/vhtml": "^2.2.4",
    "@types/video.js": "^7.3.50",
    "@vitest/coverage-c8": "^0.28.4",
    "command-line-args": "^5.2.1",
    "copyfiles": "^2.4.1",
    "esbuild-register": "^3.3.2",
    "happy-dom": "^8.2.2",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5",
    "upath": "^2.0.1",
    "vhtml": "^2.2.0",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vite-plugin-image-optimizer": "^1.1.0"
  },
  "volta": {
    "node": "19.6.0"
  }
}
