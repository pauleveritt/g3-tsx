{
  "name": "g3-tsx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run-script clean && node --require esbuild-register node_modules/.bin/eleventy --serve --watch",
    "incremental": "node --require esbuild-register node_modules/.bin/eleventy --incremental --serve --watch",
    "debug-start": "DEBUG=Eleventy* node --require esbuild-register node_modules/.bin/eleventy --serve --watch",
    "build": "npm run-script clean && node --require esbuild-register node_modules/.bin/eleventy",
    "debug-build": "DEBUG=Eleventy* node --require esbuild-register node_modules/.bin/eleventy",
    "clean": "rm -rf _site && rm -rf .11ty-vite",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-vite": "^2.0.0-canary.3",
    "@sinclair/typebox": "^0.25.21",
    "@testing-library/dom": "^8.20.0",
    "@types/markdown-it": "^12.2.3",
    "@types/vhtml": "^2.2.4",
    "@types/video.js": "^7.3.50",
    "@vitest/coverage-c8": "^0.28.4",
    "esbuild-register": "^3.3.2",
    "happy-dom": "^8.2.2",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5",
    "vhtml": "^2.2.0",
    "vite": "^4.0.4",
    "vitest": "^0.28.3"
  },
  "volta": {
    "node": "19.6.0"
  }
}
